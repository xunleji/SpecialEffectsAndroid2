<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" 
	android:layout_width="fill_parent"
	android:background="#ffffff"
	android:layout_height="fill_parent">
	
	<com.example.androidsdk.jar.customprogressbar.MyProgress 
		android:id="@+id/progressBar" 
		android:layout_width="fill_parent" 
		android:layout_height="30dp"
		style="?android:attr/progressBarStyleHorizontal" 
		android:layout_centerHorizontal="true" 
		android:max="100"/>
	
	<Button android:id="@+id/btn_start" 
	    android:layout_marginTop="20dp"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:text="开始"/>
	
	<ProgressBar 
	    android:id="@+id/progresscus"
	    android:layout_marginTop="20dp"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:indeterminate="true"
	    android:indeterminateDrawable="@drawable/progresscu"
	    style="?android:attr/progressBarStyleHorizontal"/>
	
	<com.example.androidsdk.jar.customprogressbar.ArcsProgress 
	    android:layout_marginTop="20dp"
		android:id="@+id/progressBar" 
		android:layout_width="fill_parent" 
		android:layout_height="30dp"
		android:layout_centerHorizontal="true" 
		android:max="100"/>

	<SeekBar
	    android:id="@+id/seekBar1"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content" />
	
</LinearLayout>
